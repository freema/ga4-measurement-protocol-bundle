{
  "name": "freema/ga4-measurement-protocol-bundle-test-54",
  "description": "Test environment for Symfony 5.4",
  "type": "project",
  "require": {
    "php": ">=8.1",
    "symfony/http-kernel": "5.4.*",
    "symfony/framework-bundle": "5.4.*",
    "symfony/yaml": "5.4.*",
    "symfony/console": "5.4.*",
    "symfony/http-client": "5.4.*",
    "psr/http-client": "^1.0",
    "psr/http-message": "^1.0",
    "nyholm/psr7": "^1.5",
    "php-http/discovery": "^1.14"
  },
  "require-dev": {
    "symfony/debug-bundle": "5.4.*",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Freema\\GA4MeasurementProtocolBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Freema\\GA4MeasurementProtocolBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "prefer-stable": true
}