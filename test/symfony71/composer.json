{
  "name": "freema/ga4-measurement-protocol-bundle-test-71",
  "description": "Test environment for Symfony 7.1",
  "type": "project",
  "require": {
    "php": ">=8.2",
    "symfony/http-kernel": "^7.1",
    "symfony/framework-bundle": "^7.1",
    "symfony/yaml": "^7.1",
    "symfony/console": "^7.1",
    "symfony/http-client": "^7.1",
    "psr/http-client": "^1.0",
    "psr/http-message": "^1.0",
    "nyholm/psr7": "^1.5",
    "php-http/discovery": "^1.14"
  },
  "require-dev": {
    "symfony/debug-bundle": "^7.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Freema\\GA4MeasurementProtocolBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Freema\\GA4MeasurementProtocolBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "prefer-stable": true
}